{
    "model": {
        "name": "VAE_Loss_2",
        "type": "ae",
        "layers": {
            "encoder": [
                {
                    "role": "input",
                    "type": "linear",
                    "in_dimension": 5000,
                    "out_dimension": 2500,
                    "activation": "silu"
                },
                {
                    "role": "hidden",
                    "type": "linear",
                    "in_dimension": 2500,
                    "out_dimension": 1250,
                    "activation": "silu"
                },
                {
                    "role": "hidden",
                    "type": "linear",
                    "in_dimension": 1250,
                    "out_dimension": 625,
                    "activation": "silu"
                },
                {
                    "role": "latent_space",
                    "type": "linear",
                    "in_dimension": 625,
                    "out_dimension": 200,
                    "activation": ""
                }
            ],
            "decoder": [
                {
                    "role": "input",
                    "type": "linear",
                    "in_dimension": 100,
                    "out_dimension": 625,
                    "activation": "silu"
                },
                {
                    "role": "hidden",
                    "type": "linear",
                    "in_dimension": 625,
                    "out_dimension": 1250,
                    "activation": "silu"
                },
                {
                    "role": "hidden",
                    "type": "linear",
                    "in_dimension": 1250,
                    "out_dimension": 2500,
                    "activation": "silu"
                },
                {
                    "role": "output",
                    "type": "linear",
                    "in_dimension": 2500,
                    "out_dimension": 5000,
                    "activation": "sig"
                }
            ]
        },
        "loss_function": "mae",
        "optimization": {
            "optimizer": "adam",
            "learning_rate": 1e-4,
            "weight_decay": 1e-3
        }
    },
    "training": {
        "batch_size": 128,
        "training_epochs": 200
    }
}